{
  "name": "dlx-studios",
  "private": true,
  "version": "1.0.0",
  "description": "AI-powered web development platform with intelligent token management and hybrid cloud architecture",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "proxy": "node local-proxy-server.js",
    "hybrid": "npm run proxy",
    "luxrig": "node luxrig-bridge.js",
    "luxrig:dev": "nodemon luxrig-bridge.js",
    "test:integration": "node test-integration.js",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "deploy:railway": "railway up",
    "docker:build": "docker build -t dlx-studios .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "check": "npm run typecheck && npm run lint && npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "web-development",
    "token-management",
    "react",
    "typescript",
    "supabase"
  ],
  "author": "DLX Studios",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "http-proxy-middleware": "^3.0.5",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
